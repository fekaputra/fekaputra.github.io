  title = {{Building Empirical Software Engineering Bodies of Knowledge with Systematic Knowledge Engineering}},
  url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84938367623&partnerID=MN8TOARS},
  year = {2014}
}
